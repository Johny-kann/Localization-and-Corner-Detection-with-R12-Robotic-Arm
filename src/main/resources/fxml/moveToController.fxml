<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.fit.cs.robotics.controller.gui.MoveToController">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="209.0" minWidth="10.0" prefWidth="99.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="236.0" minWidth="10.0" prefWidth="120.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="378.0" minWidth="10.0" prefWidth="127.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="378.0" minWidth="10.0" prefWidth="109.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="151.0" minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="139.0" minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="106.0" minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="home" mnemonicParsing="false" text="HOME" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="front" mnemonicParsing="false" text="Front" GridPane.columnIndex="1" />
            <Button fx:id="back" mnemonicParsing="false" text="Back" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="left" mnemonicParsing="false" text="Left" GridPane.rowIndex="1" />
            <Button fx:id="right" mnemonicParsing="false" text="Right" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Button fx:id="up" mnemonicParsing="false" text="Up" GridPane.columnIndex="3" />
            <Button fx:id="down" mnemonicParsing="false" text="Down" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Button fx:id="twist" mnemonicParsing="false" text="Twist">
               <graphic>
                  <TextField fx:id="twist_text" prefHeight="12.0" prefWidth="48.0">
                     <font>
                        <Font size="9.0" />
                     </font></TextField>
               </graphic></Button>
            <Button fx:id="wrist" mnemonicParsing="false" text="Wrist" GridPane.columnIndex="2">
               <graphic>
                  <TextField fx:id="wrist_text" prefHeight="11.0" prefWidth="44.0">
                     <font>
                        <Font size="9.0" />
                     </font></TextField>
               </graphic></Button>
            <TextField fx:id="step" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label text="Move To" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</StackPane>
