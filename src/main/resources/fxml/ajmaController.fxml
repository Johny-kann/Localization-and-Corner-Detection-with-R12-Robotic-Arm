<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="281.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.fit.cs.robotics.controller.gui.AJmaController">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="104.0" minWidth="10.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="waist_left" mnemonicParsing="false" text="Waist Left" GridPane.rowIndex="2" />
            <Button fx:id="waist_right" mnemonicParsing="false" text="Waist Right" GridPane.columnIndex="3" GridPane.rowIndex="2" />
            <Button fx:id="shoulder_down" mnemonicParsing="false" text="Shd Dwn" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Button fx:id="shoulder_up" mnemonicParsing="false" text="Shld Up" GridPane.columnIndex="2" />
            <Button fx:id="elb_down" mnemonicParsing="false" text="Elb Dwn" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="elb_up" mnemonicParsing="false" text="Elb Up" GridPane.columnIndex="1" />
            <Button fx:id="twist" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Twist">
               <graphic>
                  <TextField fx:id="twist_text" alignment="BOTTOM_LEFT" prefWidth="47.0">
                     <font>
                        <Font size="9.0" />
                     </font></TextField>
               </graphic>
            </Button>
            <Button fx:id="wrist" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Wrist" GridPane.columnIndex="3">
               <graphic>
                  <TextField fx:id="wrist_field" alignment="BOTTOM_LEFT" prefWidth="47.0">
                     <font>
                        <Font size="9.0" />
                     </font></TextField>
               </graphic>
            </Button>
            <TextField fx:id="step" text="step" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            <Label text="AJMA" GridPane.rowIndex="1" />
            <Button fx:id="home" mnemonicParsing="false" text="Home" GridPane.columnIndex="3" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</StackPane>
